<%= form_with model: @event do |form| %>
  <%=  form.label :title %>
  <%=  form.text_field :title %><br><br>

  <%= form.label :location %>
  <%= form.text_field :location %><br><br>

  <%= form.label :body %>
  <%= form.text_area :body %><br><br>

  <%= form.label :date %>
  <%= form.text_field :date %><br><br>

  <%= form.submit %>
<% end %>